@font-face
    font-family 'icomoon'
    src  url('../fonts/icomoon.eot?bd8jm1')
    src  url('../fonts/icomoon.eot?bd8jm1#iefix') format('embedded-opentype'),
        url('../fonts/icomoon.ttf?bd8jm1') format('truetype'),
        url('../fonts/icomoon.woff?bd8jm1') format('woff'),
        url('../fonts/icomoon.svg?bd8jm1#icomoon') format('svg')
    font-weight normal
    font-style normal

[class^="icon-"], [class*=" icon-"]
    font-family 'icomoon' !important
    speak none
    font-style normal
    font-weight normal
    font-variant normal
    text-transform none
    line-height 1
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale

